<div class="contain-all">
  <h1><%= @question.text %></h1>
  <h2>Battle Results</h2>
  <ul  class="font-change">
    <li>
      <% if @win == true %>
        <%= "You have won the battle!" %>
      <% else %>
        <%= "You were forced to flee!" %>
      <% end %>
    </li>
    <li><%= @character.current_health %></li>
    <li class="no-margin-box">
      <% 10.times do %>
        <button class="health-bar" ></button>
      <% end %>
    </li>
  </ul>
  <%= link_to "Back to Adventure", maps_path, class: "create-btn nes-btn is-warning font-change", style: "text-decoration: none" %>
</div>
<%# Apply the same scss with questions>show %>
